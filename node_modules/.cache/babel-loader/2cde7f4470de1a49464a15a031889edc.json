,EAAA;AACA,UAAA,QAAA,GAAA,KAAA,IAAA;AACA,MAAA,QAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,KAAA,GAAA,GAAA,CAAA,KAAA;AACA,QAAA,SAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAHA;AAIA,MAAA,QAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,OAAA,GAAA,GAAA,CAAA,OAAA;AACA,QAAA,WAAA,CAAA,IAAA,CAAA,OAAA;AACA,OAHA;AAIA,MAAA,GAAA,CAAA,OAAA,CAAA,GAAA,SAAA;AACA,MAAA,GAAA,CAAA,SAAA,CAAA,GAAA,WAAA;AACA,aAAA,GAAA;AACA;AAhCA,GAbA;AAgDA,EAAA,KAAA,EAAA,EAhDA;AAkDA,EAAA,OAlDA,qBAkDA;AACA,SAAA,IAAA;AACA,SAAA,cAAA;AACA,SAAA,oBAAA;AACA,GAtDA;AAwDA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,0BACA;AACA,WAAA,MAAA,CAAA,KAAA,GAAA,MAAA,CAAA,UAAA;;AACA,UAAA,KAAA,MAAA,CAAA,KAAA,GAAA,GAAA,EAAA;AACA,aAAA,QAAA,GAAA,KAAA;AACA,OAFA,MAEA;AACA,aAAA,QAAA,GAAA,IAAA;AACA;AACA,KARA;AAUA,IAAA,mBAVA,iCAUA;AACA,UAAA,KAAA,SAAA,MAAA,IAAA,EAAA;AACA,aAAA,cAAA,GAAA,KAAA;AACA,OAFA,MAEA;AACA,aAAA,cAAA,GAAA,IAAA;AACA;AACA,KAhBA;AAkBA,IAAA,aAlBA,2BAkBA;AACA,WAAA,KAAA,GAAA,mBAAA,KAAA,CAAA,KAAA,KAAA,CAAA,CAAA,IAAA,EAAA,EAAA,GAAA,CAAA,UAAA,CAAA,EAAA,CAAA;AAAA,eAAA,CAAA;AAAA,OAAA,CAAA;AACA,KApBA;AAsBA,IAAA,UAtBA,wBAsBA;AACA,WAAA,KAAA,GAAA,EAAA;AACA;AAxBA,GAxDA;AAmFA,EAAA,OAnFA,qBAmFA;AACA,SAAA,mBAAA,GADA,CAEA;AACA;AACA,GAvFA;AAyFA,EAAA,SAzFA,uBAyFA;AACA,IAAA,MAAA,CAAA,mBAAA,CAAA,QAAA,EAAA,KAAA,YAAA;AACA;AA3FA,CAAA","sourcesContent":["<template>\r\n  <v-container fluid>\r\n    <v-card v-if=\"isMobileDevice\" id=\"tab\" ref=\"tab\">\r\n    <v-toolbar\r\n      color=\"indigo darken-4\"\r\n      dark\r\n      flat\r\n    >\r\n\r\n      <template v-slot:extension>\r\n        <v-tabs\r\n          v-model=\"tabs\"\r\n          centered\r\n        >\r\n          <v-tab\r\n            v-for=\"(n) in dataTabDisplay.title\"\r\n            :key=\"n\"\r\n          >\r\n            {{ n }}\r\n          </v-tab>\r\n        </v-tabs>\r\n      </template>\r\n    </v-toolbar>\r\n\r\n    <v-tabs-items v-model=\"tabs\">\r\n        <v-tab-item\r\n        v-for=\"(n) in dataTabDisplay.content\"\r\n        :key=\"n\">\r\n        <v-card flat color=\"deep-purple lighten-3\">\r\n          <v-card-text class=\"headline\">\r\n            {{ n }}\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-tab-item>\r\n    </v-tabs-items>\r\n  </v-card>\r\n\r\n    <br/>\r\n    \r\n<v-container fluid>\r\n  <v-row v-if=\"!isMobileDevice\" id=\"accordion\" ref=\"accordion\">\r\n    <v-expansion-panels accordion focusable v-model=\"panel\">\r\n      <v-btn @click=\"clearPanel\" block color=\"pink\" dark>close</v-btn>\r\n      <v-expansion-panel\r\n        v-for=\"(item,i) in dataAccordionDisplay\"\r\n        :key=\"i\"\r\n      >\r\n        <v-expansion-panel-header color=\"cyan accent-2\" class=\"headline\">{{ item.title }}</v-expansion-panel-header>\r\n        <v-expansion-panel-content color=\"cyan lighten-5\">{{ item.content }}</v-expansion-panel-content>\r\n      </v-expansion-panel>\r\n    </v-expansion-panels>\r\n  </v-row>\r\n  </v-container>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    name: 'TaskTwo',\r\n\r\n    data: () => ({\r\n      tabs: null,\r\n      window: {\r\n            width: 0\r\n        },\r\n      isMobileDevice: false,\r\n      item: 1,\r\n      panel: []\r\n    }),\r\n\r\n    computed: {\r\n      data(){\r\n        return this.$store.getters['datastore/data'];\r\n      },\r\n\r\n      dataAccordionDisplay(){\r\n          const dataAcc = [];\r\n          const dataProc = this.data;\r\n          dataProc.forEach(function(obj) {\r\n            const title = obj.title;\r\n            const content = obj.content;\r\n            dataAcc.push({'title': title, 'content': content})\r\n          });\r\n          return dataAcc;\r\n      },\r\n\r\n      dataTabDisplay(){\r\n          const dataTitle = [];\r\n          const dataContent = [];\r\n          const arr = [];\r\n          const dataProc = this.data;\r\n          dataProc.forEach(function(obj) {\r\n            const title = obj.title;\r\n            dataTitle.push(title)\r\n          });\r\n          dataProc.forEach(function(obj) {\r\n            const content = obj.content;\r\n            dataContent.push(content)\r\n          });\r\n          arr['title'] =  dataTitle\r\n          arr['content'] =  dataContent\r\n          return arr;\r\n      }\r\n    },\r\n\r\n    watch: {},\r\n\r\n    mounted(){\r\n      this.data;\r\n      this.dataTabDisplay;\r\n      this.dataAccordionDisplay;\r\n    },\r\n\r\n    methods: {\r\n      windowResize() {\r\n            this.window.width = window.innerWidth;\r\n            if(this.window.width < 415) {\r\n                this.isMobile = false;\r\n            } else {\r\n                this.isMobile = true;\r\n            }\r\n      },\r\n\r\n      dataholderSelection() {\r\n        if(this.$isMobile() == true){\r\n          this.isMobileDevice = false;\r\n        } else {\r\n          this.isMobileDevice = true;\r\n        }\r\n      },\r\n\r\n      selectedPanel () {\r\n        this.panel = [...Array(this.items).keys()].map((k, i) => i)\r\n      },\r\n\r\n      clearPanel () {\r\n        this.panel = []\r\n      }\r\n    },\r\n\r\n    created() {\r\n        this.dataholderSelection();\r\n        //window.addEventListener('resize', this.windowResize);\r\n        //this.windowResize();\r\n    },\r\n\r\n    destroyed() {\r\n        window.removeEventListener('resize', this.windowResize);\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n  #dataholder {\r\n    border-radius: 20px;\r\n  }\r\n</style>"],"sourceRoot":"src/views"}]}